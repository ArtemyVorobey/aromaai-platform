
{
  "openapi": "3.1.0",
  "info": {
    "title": "AromaAI Platform",
    "version": "0.1.0"
  },
  "paths": {
    "/": {
      "get": {
        "summary": "Read Root",
        "operationId": "read_root__get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": { "type": "string" }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/predict": {
      "post": {
        "summary": "Predict",
        "operationId": "predict_predict_post",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PredictRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PredictResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/models": {
      "get": {
        "summary": "Get Models",
        "operationId": "get_models_get",
        "responses": {
          "200": {
            "description": "List of models",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModelsResponse"
                }
              }
            }
          }
        }
      }
    },
    "/history": {
      "get": {
        "summary": "Get Prediction History",
        "operationId": "get_history_get",
        "parameters": [
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": { "type": "integer", "minimum": 1, "maximum": 100 }
          }
        ],
        "responses": {
          "200": {
            "description": "History Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HistoryResponse"
                }
              }
            }
          }
        }
      },
      "delete": {
        "summary": "Clear Prediction History",
        "operationId": "clear_history_delete",
        "responses": {
          "200": {
            "description": "Status OK",
            "content": {
              "application/json": {
                "schema": { "type": "object" }
              }
            }
          }
        }
      }
    },
    "/validate": {
      "post": {
        "summary": "Validate Molecule",
        "operationId": "validate_molecule_post",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ValidateRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Validation Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ValidateResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "PredictRequest": {
        "type": "object",
        "required": ["molecule"],
        "properties": {
          "molecule": { "type": "string", "example": "C6H6" }
        }
      },
      "PredictResponse": {
        "type": "object",
        "properties": {
          "input": { "type": "string" },
          "predicted_path": { "type": "string" },
          "confidence": { "type": "number", "format": "float" }
        }
      },
      "ModelInfo": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "description": { "type": "string" }
        }
      },
      "ModelsResponse": {
        "type": "object",
        "properties": {
          "models": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/ModelInfo" }
          }
        }
      },
      "HistoryItem": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "molecule": { "type": "string" },
          "predicted_path": { "type": "string" },
          "confidence": { "type": "number", "format": "float" },
          "timestamp": { "type": "string", "format": "date-time" }
        }
      },
      "HistoryResponse": {
        "type": "object",
        "properties": {
          "predictions": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/HistoryItem" }
          }
        }
      },
      "ValidateRequest": {
        "type": "object",
        "required": ["molecule"],
        "properties": {
          "molecule": { "type": "string" }
        }
      },
      "ValidateResponse": {
        "type": "object",
        "properties": {
          "molecule": { "type": "string" },
          "valid": { "type": "boolean" },
          "message": { "type": "string" }
        }
      },
      "ValidationError": {
        "type": "object",
        "required": ["loc", "msg", "type"],
        "properties": {
          "loc": {
            "type": "array",
            "items": { "anyOf": [ { "type": "string" }, { "type": "integer" } ] }
          },
          "msg": { "type": "string" },
          "type": { "type": "string" }
        }
      },
      "HTTPValidationError": {
        "type": "object",
        "properties": {
          "detail": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/ValidationError" }
          }
        }
      }
    }
  }
}




